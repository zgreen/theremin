{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "A webpack boilerplate with corresponing npm dependencies, and an eslint config.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --content-base --inline --hot -d",
    "build": "webpack --config webpack.config.js --devtool source-map --output-pathinfo -p",
    "standard": "standard"
  },
  "author": "Zach Green",
  "license": "ISC",
  "devDependencies": {
    "arraybuffer-loader": "^0.2.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015": "^6.13.2",
    "base64-arraybuffer": "^0.1.5",
    "css-loader": "^0.23.1",
    "dom-loader": "^1.0.4",
    "eslint": "^3.3.0",
    "eslint-config-standard-react": "^4.0.1",
    "eslint-loader": "^1.5.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "json-loader": "^0.5.4",
    "postcss": "^5.1.2",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.10.0",
    "postcss-nested": "^1.0.0",
    "promise-loader": "^1.0.0",
    "standard": "^8.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
